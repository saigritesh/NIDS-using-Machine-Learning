<%- include('partials/header') %>
<%- include('partials/loadhead') %>
<body onload="myFunction()" style="margin:0;">
  <div id="loader"></div>
  <div style="display:none;" id="myDiv" class="animate-bottom">
    <div class="container-fluid">
      <div class="row" style="color: #fff;">

        <!-- KNN Card -->
        <div class="card text-center" style="width: 35rem; margin: 30px; background-color: #71C9CE;">
          <div class="card-body">
            <h4 class="card-title">K-Nearest-Neighbor (KNN)</h4>
            <p class="card-text"><%= p_knn_bin_cls || 'N/A' %></p>
            <p class="card-text"><%= p_knn_mul_cls || 'N/A' %></p>
            <p class="card-text"><%= p_knn_desc || 'No description available.' %></p>
            <p class="card-text">KNN Binary Class Accuracy = <%= p_knn_bin_acc || '0.00' %></p>
            <p class="card-text">KNN Multi Class Accuracy = <%= p_knn_mul_acc || '0.00' %></p>
            <a class="btn btn-info" href="/knn_bin_table" role="button">Binary-Class Classification Table</a>
            <a class="btn btn-info" href="/knn_table" role="button">Multi-Class Classification Table</a>
          </div>
        </div>

        <!-- Random Forest Card -->
        <div class="card text-center" style="width: 35rem; margin: 30px; background-color: #71C9CE;">
          <div class="card-body">
            <h4 class="card-title">Random Forest</h4>
            <p class="card-text"><%= p_rf_bin_cls || 'N/A' %></p>
            <p class="card-text"><%= p_rf_mul_cls || 'N/A' %></p>
            <p class="card-text"><%= p_rf_desc || 'No description available.' %></p>
            <p class="card-text">Random Forest Binary Class Accuracy = <%= p_rf_bin_acc || '0.00' %></p>
            <p class="card-text">Random Forest Multi Class Accuracy = <%= p_rf_mul_acc || '0.00' %></p>
            <a class="btn btn-info" href="/rf_bin_table" role="button">Binary-Class Classification Table</a>
            <a class="btn btn-info" href="/rf_table" role="button">Multi-Class Classification Table</a>
          </div>
        </div>

        <!-- CNN Card -->
        <div class="card text-center" style="width: 35rem; margin: 30px; background-color: #71C9CE;">
          <div class="card-body">
            <h4 class="card-title">Convolutional Neural Network (CNN)</h4>
            <p class="card-text"><%= p_cnn_bin_cls || 'N/A' %></p>
            <p class="card-text"><%= p_cnn_mul_cls || 'N/A' %></p>
            <p class="card-text"><%= p_cnn_desc || 'No description available.' %></p>
            <p class="card-text">CNN Binary Class Accuracy = <%= p_cnn_bin_acc || '0.00' %></p>
            <p class="card-text">CNN Multi Class Accuracy = <%= p_cnn_mul_acc || '0.00' %></p>
            <a class="btn btn-info" href="/cnn_bin_table" role="button">Binary-Class Classification Table</a>
            <a class="btn btn-info" href="/cnn_table" role="button">Multi-Class Classification Table</a>
          </div>
        </div>

        <!-- LSTM Card -->
        <div class="card text-center" style="width: 35rem; margin: 30px; background-color: #71C9CE;">
          <div class="card-body">
            <h4 class="card-title">Long Short Term Memory (LSTM)</h4>
            <p class="card-text"><%= p_lstm_bin_cls || 'N/A' %></p>
            <p class="card-text"><%= p_lstm_mul_cls || 'N/A' %></p>
            <p class="card-text"><%= p_lstm_desc || 'No description available.' %></p>
            <p class="card-text">LSTM Binary Class Accuracy = <%= p_lstm_bin_acc || '0.00' %></p>
            <p class="card-text">LSTM Multi Class Accuracy = <%= p_lstm_mul_acc || '0.00' %></p>
            <a class="btn btn-info" href="/lstm_bin_table" role="button">Binary-Class Classification Table</a>
            <a class="btn btn-info" href="/lstm_table" role="button">Multi-Class Classification Table</a>
          </div>
        </div>

        <!-- Action Buttons -->
        <div class="login-box" style="margin: 30px;">
          <a class="btn btn-success" href="/submit">Predict Again!</a>
          <a class="btn btn-danger" href="/logout">LOGOUT</a>
        </div>

      </div>
    </div>
    <%- include('partials/footer') %>
  </div>
  <script>
    var myVar;
    function myFunction() {
      myVar = setTimeout(showPage, 13000);
    }
  </script>
  <%- include('partials/loadfoot') %>
